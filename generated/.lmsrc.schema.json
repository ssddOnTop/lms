{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "Algorithm": {
      "enum": [
        "SHA1",
        "Sha256",
        "Sha512"
      ],
      "type": "string"
    },
    "AuthInfo": {
      "properties": {
        "aes_key": {
          "type": "string"
        },
        "auth_url": {
          "type": "string"
        },
        "totp": {
          "$ref": "#/definitions/TotpSettings"
        }
      },
      "type": "object"
    },
    "Server": {
      "properties": {
        "host": {
          "type": [
            "string",
            "null"
          ]
        },
        "port": {
          "format": "uint16",
          "minimum": 0.0,
          "type": [
            "integer",
            "null"
          ]
        },
        "workers": {
          "format": "uint",
          "minimum": 0.0,
          "type": [
            "integer",
            "null"
          ]
        }
      },
      "type": "object"
    },
    "TotpSettings": {
      "properties": {
        "algo": {
          "anyOf": [
            {
              "$ref": "#/definitions/Algorithm"
            },
            {
              "type": "null"
            }
          ]
        },
        "digits": {
          "format": "uint",
          "minimum": 0.0,
          "type": [
            "integer",
            "null"
          ]
        },
        "period": {
          "format": "uint64",
          "minimum": 0.0,
          "type": [
            "integer",
            "null"
          ]
        },
        "totp_secret": {
          "type": "string"
        }
      },
      "type": "object"
    }
  },
  "properties": {
    "auth": {
      "$ref": "#/definitions/AuthInfo"
    },
    "server": {
      "$ref": "#/definitions/Server"
    }
  },
  "title": "Config",
  "type": "object"
}